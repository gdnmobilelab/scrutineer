    "start-server": "NODE_ENV=stage PORT=3001 forever start scrutineer-app/bin/www",
    "stop-server": "forever stop scrutineer-app/bin/www",
    "start-results": "NODE_ENV=stage forever start scrutineer-app/scrutineer-results/index.js",
    "stop-results": "forever stop scrutineer-app/scrutineer-results/index.js",
    "start-all": "npm run start-server && npm run start-results",
    "stop-all": "npm run stop-server && npm run stop-results",
